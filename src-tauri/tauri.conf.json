{
  "productName": "Rewind",
  "version": "1.4.0",
  "identifier": "com.rewind.app",
  "build": {
    "beforeDevCommand": "pnpm dev:frontend",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "pnpm build:frontend",
    "frontendDist": "../frontend/dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Rewind",
        "width": 1200,
        "height": 800,
        "minWidth": 900,
        "minHeight": 600
      }
    ]
  },
  "bundle": {
    "createUpdaterArtifacts": true,
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": {
      "icons/Assets.car": "Assets.car"
    },
    "macOS": {
      "infoPlist": "./Info.plist"
    }
  },
  "plugins": {
    "deep-link": {
      "desktop": {
        "schemes": ["rewind"]
      }
    },
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEI1QTM0Qzc2QjUwQkQ5RDMKUldUVDJRdTFka3lqdFRsZGM2R0hJSHVGOUx3TzIrN3NkYzlPWlFrSGJOTTRZa3dxUjcwM3BoTkcK",
      "endpoints": [
        "https://github.com/nickkhg/rewind/releases/latest/download/latest.json"
      ]
    }
  }
}
